<#Canton>
  rml:logicalSource [
    rml:source "eCH0071_150101.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/eCH-0071:nomenclature/cantons/canton"
  ]; 

  rr:subjectMap [
    rr:template "http://data.admin.ch/id/canton/{cantonAbbreviation}";
    rr:class hgv:Canton ;
  ];

    rr:predicateObjectMap [
    rr:predicate hgv:cantonAbbreviation;
    rr:objectMap [
      rml:reference "/canton/cantonAbbreviation";
    ]
 ] ;

    rr:predicateObjectMap [
    rr:predicate hgv:id;
    rr:objectMap [
      rml:reference "/canton/cantonId";
      rr:datatype xsd:integer ;
    ]
 ] ;

    rr:predicateObjectMap [
    rr:predicate hgv:longName;
    rr:objectMap [
      rml:reference "/canton/cantonLongName";
    ]
 ] ;

   rr:predicateObjectMap [
    rr:predicate hgv:date;
    rr:objectMap [
      rml:reference "/canton/cantonDateOfChange";
      rr:datatype xsd:date ;
    ]
 ] .

